                  <div class="col-md-6 col-md-push-6">
                    <!-- FOOTER MENU : begin -->
                    <nav class="footer-menu">
                      <ul>
                        <li><a href="{{ path('nadine_coiffure_front_home') }}">Accueil</a></li>
